{
  "name": "alx",
  "description": "flux framework",
  "version": "0.0.3",
  "author": "cotto",
  "ava": {
    "files": [
      "test/compiled/test/**/*.test.js"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "bugs": {
    "url": "https://github.com/cotto89/alx/issues"
  },
  "dependencies": {
    "lodash.merge": "^4.6.0"
  },
  "devDependencies": {
    "@types/node": "^6.0.46",
    "@types/react": "^0.14.43",
    "@types/react-dom": "^0.14.18",
    "@types/sinon": "^1.16.31",
    "autoprefixer": "^6.5.1",
    "ava": "^0.16.0",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.10.1",
    "npm-run-all": "^3.1.1",
    "postcss-loader": "^1.0.0",
    "power-assert": "^1.4.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.6",
    "ts-loader": "^0.9.5",
    "tslint": "^3.15.1",
    "typescript": "^2.0.6",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.15.0",
    "webpack-notifier": "^1.4.1"
  },
  "files": [
    "index.js",
    "lib"
  ],
  "keywords": [
    "flux"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cotto89/alx.git"
  },
  "scripts": {
    "build": "npm-run-all lint clean -p build:dist build:example -s clean:dist:docs",
    "build:dist": "tsc",
    "build:example": "NODE_ENV=production webpack -p",
    "build:test": "tsc -p test/tsconfig.json",
    "clean": "npm-run-all clean:** -p",
    "clean:dist": "rimraf ./dist",
    "clean:dist:docs": "rimraf ./dist/docs",
    "clean:example": "rimraf ./docs/example/**/bundle.**",
    "clean:test": "rimraf ./test/compiled",
    "lint": "tslint lib/**/*.ts",
    "start": "npm run watch:dist",
    "start:test": "NODE_ENV=test ava --verbose",
    "test": "run-s build:test start:test clean:test",
    "watch:dist": "tsc -w",
    "watch:example": "webpack-dev-server",
    "watch:test": "npm-run-all build:test -p 'build:test -- -w' 'start:test -- -w'"
  }
}